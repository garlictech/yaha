{
  "version": 2,
  "projects": {
    "backend": {
      "root": "apps/backend",
      "sourceRoot": "apps/backend/src",
      "projectType": "application",
      "prefix": "backend",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/backend",
            "parallel": false,
            "commands": [
              "yarn sls package --stage='{args.stage}'",
              "yarn sst cdk synth"
            ]
          },
          "outputs": [
            "apps/backend/cdk.out",
            "apps/backend/.build",
            "apps/backend/.serverless"
          ]
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/backend",
            "parallel": false,
            "commands": [
              {
                "command": "yarn sst deploy --stage='{args.stage}'"
              },
              {
                "command": "cd ../.. && yarn ts-node ./tools/fetch-configuration.ts '{args.app}' '{args.stage}'"
              }
            ]
          }
        },
        "remove": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/backend",
            "parallel": false,
            "commands": [
              {
                "command": "yarn sst remove --stage='{args.stage}'"
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/backend/**/*.ts"],
            "cache": true
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/backend"],
          "options": {
            "jestConfig": "apps/backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["type:backend", "platform:nodejs"]
    },
    "cicd": {
      "root": "apps/cicd",
      "sourceRoot": "apps/cicd/src",
      "projectType": "application",
      "prefix": "cicd",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [
            "apps/cicd/cdk.out",
            "apps/cicd/.build",
            "apps/cicd/.serverless"
          ],
          "options": {
            "outputPath": "dist/apps/cicd",
            "main": "apps/cicd/src/main.ts",
            "tsConfig": "apps/cicd/tsconfig.app.json",
            "assets": ["apps/cicd/src/assets"]
          }
        },
        "deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/cicd",
            "commands": [
              {
                "command": "yarn sst deploy"
              }
            ]
          }
        },
        "remove": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/cicd",
            "commands": [
              {
                "command": "yarn sst remove"
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/cicd/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/cicd"],
          "options": {
            "jestConfig": "apps/cicd/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["type:backend", "platform:nodejs"]
    },
    "mobile_app": {
      "root": "apps/mobile_app",
      "sourceRoot": "apps/mobile_app/src",
      "projectType": "application",
      "targets": {
        "analyze": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter analyze",
            "cwd": "apps/mobile_app"
          }
        },
        "clean": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter clean",
            "cwd": "apps/mobile_app"
          }
        },
        "format": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter format apps/mobile_app/*",
            "cwd": "apps/mobile_app"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter test",
            "cwd": "apps/mobile_app"
          }
        },
        "test-ci": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "docker run --rm -v ${PWD}:/build -v /tmp/x:/root/.pub-cache --workdir /build 697486207432.dkr.ecr.us-east-1.amazonaws.com/yaha-flutter flutter test",
            "cwd": "apps/mobile_app"
          }
        },
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter analyze",
            "cwd": "apps/mobile_app"
          }
        },
        "lint-ci": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "docker run --rm -v ${PWD}:/build -v /tmp/x:/root/.pub-cache --workdir /build  697486207432.dkr.ecr.us-east-1.amazonaws.com/yaha-flutter flutter analyze",
            "cwd": "apps/mobile_app"
          }
        },
        "assemble": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter assemble",
            "cwd": "apps/mobile_app"
          }
        },
        "attach": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter attach",
            "cwd": "apps/mobile_app"
          }
        },
        "drive": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter drive",
            "cwd": "apps/mobile_app"
          }
        },
        "genL10n": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter gen-l10n",
            "cwd": "apps/mobile_app"
          }
        },
        "install": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter install",
            "cwd": "apps/mobile_app"
          }
        },
        "run": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter run",
            "cwd": "apps/mobile_app"
          }
        },
        "buildAar": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter build aar",
            "cwd": "apps/mobile_app"
          }
        },
        "buildApk": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter build apk",
            "cwd": "apps/mobile_app"
          }
        },
        "buildApk-ci": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "docker run --rm -v ${PWD}:/build -v /tmp/x:/root/.pub-cache --workdir /build 697486207432.dkr.ecr.us-east-1.amazonaws.com/yaha-flutter flutter build apk",
            "cwd": "apps/mobile_app"
          }
        },
        "buildAppbundle": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter build appbundle",
            "cwd": "apps/mobile_app"
          }
        },
        "buildBundle": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter build bundle",
            "cwd": "apps/mobile_app"
          }
        },
        "buildIos": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter build ios",
            "cwd": "apps/mobile_app"
          }
        },
        "buildIosFramework": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter build ios-framework",
            "cwd": "apps/mobile_app"
          }
        },
        "buildIpa": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "flutter build ipa",
            "cwd": "apps/mobile_app"
          }
        },
        "graphql-codegen": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "command": "flutter pub run build_runner build --delete-conflicting-outputs",
            "cwd": "apps/mobile_app"
          }
        },
        "graphql-codegen-ci": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "parallel": false,
            "command": "docker run --rm -v ${PWD}:/build -v /tmp/x:/root/.pub-cache --workdir /build  ${AWS_ACCOUNT}.dkr.ecr.eu-west-1.amazonaws.com/anyupp-flutter flutter pub run build_runner build --delete-conflicting-outputs",
            "cwd": "apps/mobile_app"
          }
        }
      },
      "tags": ["type:application", "platform:flutter"]
    },
    "neo4j-gql-api": {
      "root": "libs/neo4j-gql/api",
      "sourceRoot": "libs/neo4j-gql/api/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/neo4j-gql/api/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/neo4j-gql/api"],
          "options": {
            "jestConfig": "libs/neo4j-gql/api/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn graphql-codegen --config tools/graphql-codegen-neo4j.yml"
              }
            ]
          },
          "outputs": ["libs/neo4j-gql/api/src/lib/generated"]
        }
      },
      "tags": []
    },
    "neo4j-gql-backend": {
      "root": "libs/neo4j-gql/backend",
      "sourceRoot": "libs/neo4j-gql/backend/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/neo4j-gql/backend/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/neo4j-gql/backend"],
          "options": {
            "jestConfig": "libs/neo4j-gql/backend/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "shared-config": {
      "root": "libs/shared/config",
      "sourceRoot": "libs/shared/config/src",
      "projectType": "library",
      "targets": {
        "config": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "yarn ts-node ./tools/fetch-configuration.ts '{args.app}' '{args.stage}'",
              "yarn ts-node ./tools/get-secrets.ts 'yaha' '{args.stage}'"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/config/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/config"],
          "options": {
            "jestConfig": "libs/shared/config/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "shared-utils": {
      "root": "libs/shared/utils",
      "sourceRoot": "libs/shared/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/utils"],
          "options": {
            "jestConfig": "libs/shared/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["type:utils", "platform:universal"]
    }
  }
}
