{
  "name": "flutter-yama-lib",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/flutter/yama-lib/src",
  "projectType": "library",
  "targets": {
    "analyze": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter analyze",
        "cwd": "libs/flutter/yama-lib"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter clean",
        "cwd": "libs/flutter/yama-lib"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dart format libs/flutter/yama-lib/*",
        "cwd": "libs/flutter/yama-lib"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter test",
        "cwd": "libs/flutter/yama-lib"
      }
    },
    "doctor": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter doctor",
        "cwd": "libs/flutter/yama-lib"
      }
    },
    "build-aar": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build aar",
        "cwd": "libs/flutter/yama-lib"
      },
      "outputs": ["{workspaceRoot}/libs/flutter/yama-lib/build"]
    },
    "build-apk": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build apk",
        "cwd": "libs/flutter/yama-lib"
      },
      "outputs": ["{workspaceRoot}/libs/flutter/yama-lib/build"]
    },
    "build-appbundle": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build appbundle",
        "cwd": "libs/flutter/yama-lib"
      },
      "outputs": ["{workspaceRoot}/libs/flutter/yama-lib/build"]
    },
    "build-bundle": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build bundle",
        "cwd": "libs/flutter/yama-lib"
      },
      "outputs": ["{workspaceRoot}/libs/flutter/yama-lib/build"]
    },
    "build-ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ios",
        "cwd": "libs/flutter/yama-lib"
      },
      "outputs": ["{workspaceRoot}/libs/flutter/yama-lib/build"]
    },
    "build-ios-framework": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ios-framework",
        "cwd": "libs/flutter/yama-lib"
      },
      "outputs": ["{workspaceRoot}/libs/flutter/yama-lib/build"]
    },
    "build-ipa": {
      "executor": "nx:run-commands",
      "options": {
        "command": "flutter build ipa",
        "cwd": "libs/flutter/yama-lib"
      },
      "outputs": ["{workspaceRoot}/libs/flutter/yama-lib/build"]
    }
  },
  "tags": []
}
