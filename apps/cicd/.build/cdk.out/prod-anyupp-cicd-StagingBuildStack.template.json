          "PrivilegedMode": false,
          "Type": "LINUX_CONTAINER"
        },
        "ServiceRole": {
          "Fn::GetAtt": [
            "BuildRoleB7C66CB2",
            "Arn"
          ]
        },
        "Source": {
          "BuildSpec": "{\n  \"version\": \"0.2\",\n  \"phases\": {\n    \"install\": {\n      \"commands\": [\n        \"chmod +x ./tools/*.sh\",\n        \"./tools/setup-aws-environment.sh\",\n        \"./tools/install-nodejs-14.sh\",\n        \"yarn --frozen-lockfile\",\n        \"npm install -g @aws-amplify/cli appcenter-cli\"\n      ]\n    },\n    \"build\": {\n      \"commands\": [\n        \"./tools/build-workspace.sh anyupp-backend staging\",\n        \"git clone https://github.com/flutter/flutter.git -b stable --depth 1 /tmp/flutter\",\n        \"yarn nx deploy crud-backend\",\n        \"yarn nx deploy anyupp-backend --stage=staging --app=anyupp-backend\",\n        \"export PATH=$PATH:/tmp/flutter/bin\",\n        \"flutter doctor\",\n        \"yarn nx buildApk anyupp-mobile\"\n      ]\n    },\n    \"post_build\": {\n      \"commands\": [\n        \"tar -cvf ${CODEBUILD_RESOLVED_SOURCE_VERSION}.tgz apps/anyupp-mobile/lib/awsconfiguration.dart\",\n        \"aws s3 cp ${CODEBUILD_RESOLVED_SOURCE_VERSION}.tgz s3://anyupp-build-artifacts-staging/\",\n        \"echo 'Pushing Android APK to appcenter'\",\n        \"./tools/publish-to-appcenter.sh staging android\",\n        \"echo 'Triggering ios app build in appcenter...'\",\n        \"./tools/trigger-appcenter-builds.sh staging ios\"\n      ]\n    }\n  },\n  \"artifacts\": {\n    \"files\": [\n      \"apps/anyupp-backend/cdk.out/**/*\"\n    ]\n  },\n  \"env\": {\n    \"secrets-manager\": {\n      \"AWS_ACCESS_KEY_ID\": \"codebuild:codebuild-aws_access_key_id\",\n      \"AWS_SECRET_ACCESS_KEY\": \"codebuild:codebuild-aws_secret_access_key\",\n      \"APP_CENTER_TOKEN\": \"codebuild:codebuild-appcenter-token\"\n    },\n    \"variables\": {\n      \"NODE_OPTIONS\": \"--unhandled-rejections=strict --max_old_space_size=8196\"\n    }\n  }\n}",
          "Type": "CODEPIPELINE"
        },
        "Cache": {
          "Modes": [
            "LOCAL_CUSTOM_CACHE"
          ],
          "Type": "LOCAL"
        },
        "EncryptionKey": {
          "Fn::GetAtt": [
            "PipelineArtifactsBucketEncryptionKey01D58D69",
            "Arn"
          ]
        }
      },
      "Metadata": {
        "aws:cdk:path": "prod-anyupp-cicd-StagingBuildStack/Build/Resource"
      }
